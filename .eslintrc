{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": [
            0
        ]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true,
        "modules": true
    },
    "plugins": [
        "react"
    ],
    "globals": {
        jQuery: true,
        $: true,
        require: true,
        console: true,
        module: true,
        __dirname: true,
        describe: true,
        it: true,
        expect: true,
        jest: true,
        beforeEach: true,
        process: true
    }
}
