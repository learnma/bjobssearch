{
  "name": "bjobsearch",
  "version": "0.0.1",
  "description": "A job serach page for blue collar jobs",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "testdbg": "node debug --harmony ./node_modules/jest-cli/bin/jest.js --runInBand",
    "build": "webpack -p --config webpack.production.config.js",
    "dev": "webpack-dev-server --devtool eval --progress --colors --hot",
    "start": "node src/server/app.js"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  },
  "author": "madhanganesh@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.11.3",
    "file-loader": "^0.8.5",
    "jest": "^0.1.40",
    "jest-cli": "^0.8.2",
    "less": "^2.5.3",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "babel-jest": "^6.0.1",
    "body-parser": "^1.14.2",
    "express": "^4.13.3",
    "font-awesome-webpack": "0.0.4",
    "history": "^1.17.0",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "morgan": "^1.6.1",
    "react": "^0.14.3",
    "react-bootstrap": "^0.28.2",
    "react-dom": "^0.14.3",
    "react-fontawesome": "^0.3.3",
    "react-router": "^1.0.3"
  }
}
